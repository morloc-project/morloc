import cppbase (zipWith, mul)
import pybase (add)
export foo

add3 x y z = add x (add y z)

foo Cpp :: "double" -> ["double"] -> ["double"] -> ["double"]
foo x = zipWith (\y -> add3 (mul x y) y)

