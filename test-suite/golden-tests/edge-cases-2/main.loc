module m (foo, bar)

type R => Str = "character"
type R => Int = "integer"
type R => (Tuple2 a b) = "list" a b

type Py => Str = "str"
type Py => Int = "int"
type Py => (Tuple2 a b) = "tuple" a b

record Person = Person
  { name :: Str
  , age :: Int
  }

record Py => Person = "dict"
  { name :: "str"
  , age :: "int"
  }

record R => Person = "list"
  { name :: "std::string"
  , age :: "int"
  }

source Py from "foo.py" ("foo")
source R from "foo.R" ("id")

id :: a -> a
foo :: Str -> Person

bar = id . foo
